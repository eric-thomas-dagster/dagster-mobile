{
  "expo": {
    "name": "Dagster+ Mobile",
    "slug": "dagster-mobile",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/dagster-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dagster.mobile",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Dagster+ Mobile uses Face ID to securely authenticate and protect your API credentials."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dagster-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dagster.mobile",
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ],
      "navigationBar": {
        "visible": true,
        "barStyle": "dark-content",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": false,
          "data": [
            {
              "scheme": "https",
              "host": "*.dagster.cloud",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "dagster.cloud",
              "pathPrefix": "/"
            },
            {
              "scheme": "http",
              "host": "*.dagster.cloud",
              "pathPrefix": "/"
            },
            {
              "scheme": "http",
              "host": "dagster.cloud",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3d630964-b73b-445f-af52-58a8e2a65b3b"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-background-task"
    ],
    "scheme": "dagster-mobile"
  }
}
